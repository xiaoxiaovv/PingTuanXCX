  background-color: #fff;
  width: 100%;
  display: none;
  z-index: 101
}

.sku_pack {
  padding: 10px;
}

.sku_pack .sku_img {
  position: absolute;
  top: -20px;
  left: 10px;
  background-color: #fff;
  border-radius: 10px;
  width: 100px;
  height: 100px;
  border: 1px solid #e4e4e4;
}

.sku_price_stock {
  height: 80px;
