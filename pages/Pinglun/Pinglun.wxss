}

.sku_num input, .sku_num text {
  display: block;
  width: 52rpx;
  height: 54rpx;
  text-align: center;
  line-height: 54rpx;
  float: right;
}
.sku_num input{
  border-top: 1px solid #e4e4e4;
  border-bottom: 1px solid #e4e4e4;
}
.sku_num text{
