page {
  height: auto;
  background: #eee;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #333;
  letter-spacing: 0;
  padding-bottom: 100rpx;
}

button {
  padding: 0;
  margin: 0;
}

button::after {
  border: none;
}

::-webkit-scrollbar {
  display: none;
}

image {
  width: 100%;
  height: 100%;
}

.header {
  width: 100%;
  height: 208rpx;
  background: #fff;
  box-sizing: border-box;
  padding: 10rpx 25rpx;
  display: flex;
  display: -webkit-flex;
  flex-flow: row wrap;
}

.height {
  height: 100%;
}

.btn_top {
  width: 218rpx;
  height: 64rpx;
  line-height: 64rpx;
  border: 0.25rpx solid #999;
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #999;
  letter-spacing: 0;
  text-align: center;
  margin-right: 10rpx;
}

.active {
  color: #ff6026;
  background: rgba(255, 68, 0, 0.05);
  border: none;
  border: 1rpx solid #ff6026;
  border-radius: 4px;
}

.pinglunqu {
  width: 100%;
  height: auto;
  padding: 10px 25rpx;
  box-sizing: border-box;
  margin-top: 10rpx;
  position: relative;
  background: #fff;
}

.no {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.user_top {
  width: 100%;
  height: 104rpx;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: space-between;
}

.img {
  width: 64rpx;
  height: 64rpx;
}

.img image {
  border-radius: 100%;
}

.username {
  flex: 40%;
  margin-left: 20rpx;
}

.date {
  font-size: 12px;
  color: #999;
}

.pingjia {
  line-height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  margin-bottom: 10rpx;
}

.tu {
  font-size: 0;
}

.tu view {
  display: inline-block;
  width: 150rpx;
  height: 150rpx;
  margin-top: 16rpx;
  margin-right: 28rpx;
  vertical-align: top;
}

.tu view:nth-child(4n) {
  margin-right: 0;
}

.shangjia {
  width: 100%;
  height: 175rpx;
  background: #eee;
  color: #666;
  margin-top: 25rpx;
  position: relative;
}

.shangjia1 {
  width: 622rpx;
  height: auto;
  min-height: 120rpx;
  padding-top: 20rpx;
  margin: auto;
}

.sanjiao {
  position: absolute;
  width: 0rpx;
  height: 0rpx;
  line-height: 0rpx;
  border-bottom: 10px solid #eee;
  border-left: 20rpx solid #fff;
  border-right: 20rpx solid #fff;
  left: 114rpx;
  top: -20rpx;
}

.zhui {
  background: #fff;
}

.zhui_top {
  line-height: 60rpx;
  overflow: hidden;
}

.zhui_bottom {
  line-height: 40rpx;
  text-align: justify;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.zhui1 {
  float: left;
  color: #ff6026;
}

.zhui2 {
  color: #999;
  float: right;
  font-size: 24rpx;
}

.bar {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 98rpx;
  background-color: #fff;
  margin-top: 30rpx;
}

.bar .bar-1 {
  float: left;
  height: 100%;
  width: 16%;
}

.bar .bar-3 {
  float: left;
  height: 100%;
  width: 33.5%;
  border-radius: 0;
}
.barClass{
  height: 140rpx !important;
}
.bar-3 button {
  padding: 0;
  margin: 0;
  border-radius: 0;
}

.bar .bar-1 image {
  width: 36rpx;
  height: 36rpx;
  margin-left: 46rpx;
  margin-top: 18rpx;
  margin-bottom:11rpx;
}

.bar .text-2 {
  font-size: 20rpx;
  text-align: center;
  height: 26rpx;
  line-height: 26rpx;
}

.bar-3 button::after {
  border-radius: 0;
}

.bar .text-1 {
  text-align: center;
  font-size: 28rpx;
}

.border-bar1, .border-bar2 {
  position: absolute;
  height: 90rpx;
  color: #eee;
  left: 16%;
  line-height: 90rpx;
  width: 3rpx;
  text-align: center;
}

.border-bar2 {
  left: 32%;
}

.my-contact {
  float: left;
  height: 100%;
  width: 17%;
  background: 0 0 !important;
  color: #fff;
}

.contact-c {
  height: 96rpx;
  display: flex;
  display: -webkit-box;
  display: -webkit-flex;
  flex: 1;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.contact-img {
  width: 36rpx;
  height: 36rpx;
}

.contact-text {
  height: 28rpx;
  bottom: 26rpx;
}

.my-contact::after {
  border: none;
}

image {
  width: 100%;
  height: 100%;
}

.sku {
  width: 100%;
  height: 100%;
