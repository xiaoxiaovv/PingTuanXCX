  font-size: 12px;
}

.sku_title {
  clear: both;
  font-size: 16px;
  font-weight: bold;
  border-top: 1px solid #e4e4e4;
  margin-top: 10px;
  padding-top: 10px;
}

.sku_btn {
  font-size: 14px;
  min-width: 50px;
  height: 35px;
  line-height: 35px;
  text-align: center;
  border-radius: 5px;
  margin: 10px;
  padding: 0px 5px;
  float: left;
  background-color: #eee;
}

.sku_red {
  background-color: #fe6026;
  color: #fff;
  border: none;
}

.sku_num text, .sku_num input {
  display: block;
  border: 1px solid #e4e4e4;
  width: 50px;
  height: 40px;
  text-align: center;
  line-height: 40px;
  float: right;
}

.sku_num {
  float: right;
  font-weight: normal;
}

.sku_buynum {
  border-bottom: 1px solid #e4e4e4;
  height: 60px;
}

