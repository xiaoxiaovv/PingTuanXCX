  height: 60px;
  background-color: #ff6026;
  color: #fff;
  position: fixed;
  bottom: 0px;
  width: 100%;
  text-align: center;
  line-height: 60px;
}

.gobuy_gray {
  background-color: #333;
}

.sku_close {
  position: absolute;
  right: 20px;
  top: 10px;
  height: 40px;
  width: 40px;
  text-align: center;
  line-height: 40px;
}

.min_wrap {
  height: 40%;
  display: none;
}

.sku_hidden {
  display: none;
}

.sku_show {
  display: block;
}

